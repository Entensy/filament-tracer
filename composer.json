{
    "name": "entensy/filament-tracer",
    "description": "Filament plugin to report exceptions and traces. Table schema is compatible with any language.",
    "keywords": [
        "entensy",
        "filament-tracer"
    ],
    "homepage": "https://github.com/entensy/filament-tracer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "AlanD20",
            "email": "aland20@pm.me",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^10.x-dev|^11.x-dev",
        "illuminate/database": "^10.x-dev|^11.x-dev",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.16",
        "spatie/laravel-ignition": "^2.0"
    },
    "require-dev": {
        "illuminate/testing": "^10.29",
        "laravel/pint": "^1.13",
        "pestphp/pest": "^2.24.0",
        "phpunit/phpunit": "^10.4.0"
    },
    "autoload": {
        "psr-4": {
            "Entensy\\FilamentTracer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Entensy\\FilamentTracer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "format": "vendor/bin/pint src tests --config pint.json"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Entensy\\FilamentTracer\\FilamentTracerServiceProvider"
            ],
            "aliases": {
                "FilamentTracer": "Entensy\\FilamentTracer\\FilamentTracerFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
